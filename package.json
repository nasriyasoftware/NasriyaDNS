{
  "name": "@nasriya/dns",
  "version": "1.0.3",
  "description": "A DNS manager for your domains",
  "main": "./dist/cjs/nasriya-dns.js",
  "module": "./dist/esm/nasriya-dns.js",
  "types": "./dist/@types/nasriya-dns.d.ts",
  "files": [
    "dist/**/*"
  ],
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "require": "./dist/cjs/nasriya-dns.js",
      "import": "./dist/esm/nasriya-dns.js",
      "types": "./dist/@types/nasriya-dns.d.ts"
    }
  },
  "publishConfig": {
    "provenance": true
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nasriyasoftware/NasriyaDNS"
  },
  "maintainers": [
    {
      "name": "Ahmad Nasriya",
      "email": "ahmad@nasriya.net",
      "url": "https://www.linkedin.com/in/ahmadnasriya/"
    }
  ],
  "scripts": {
    "build": "npm run build:esm && npm run build:cjs && npm run copyFiles",
    "build:esm": "tsc --project tsconfig.esm.json",
    "build:cjs": "tsc --project tsconfig.cjs.json",
    "copyFiles": "ns-copySrc",
    "test": "jest"
  },
  "author": "Nasriya Software, LLC.",
  "keywords": [
    "dns",
    "domain",
    "ip",
    "A record",
    "CNAME"
  ],
  "license": "Nasriya License",
  "dependencies": {
    "tldts": "^6.1.11"
  },
  "bugs": {
    "url": "https://github.com/nasriyasoftware/NasriyaDNS/issues"
  },
  "homepage": "https://github.com/nasriyasoftware/NasriyaDNS",
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.12.7",
    "jest": "^29.7.0",
    "nasriya-copyfiles": "github:nasriyasoftware/CopyFiles",
    "nasriya-modularizejs": "github:nasriyasoftware/ModularizeJS",
    "ts-jest": "^29.1.2",
    "tsup": "^8.1.0",
    "typescript": "^5.4.5"
  }
}
